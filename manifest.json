{
  "manifest_version": 3,
  "name": "아주대 캘린더",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["scripting", "tabs"],
  "host_permissions": ["*://eclass2.ajou.ac.kr/*"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [{
    "matches": ["*://eclass2.ajou.ac.kr/ultra/*"],
    "js": ["content.js"]
  }]
}
